      ,[TVA] as'TVA'
      ,[Quantite_TOTAL] as' ' 
      ,[price_vente1] as'  '
      ,[price_vente2] as'  '
      ,[price_min] as'  '
      ,[vente_apres_expiration] as'   '
      ,[stocke_negatif] as'  -'
      ,[Quantite_vente] as' '
