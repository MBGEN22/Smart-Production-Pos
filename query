CREATE PROC [dbo].[get_fctr_on_state]
@Date date
AS
SELECT 
    [NMR_FACTURE] AS ' ',
    [DATE] AS '',
    CONVERT(VARCHAR(8), [HEURE], 108) AS '',
    TB_CLIENT.NAME + ' ' + TB_CLIENT.PRENAME AS '',
    [TOTAL_FACURE_TTC] AS '  TTC',
    [VERSEMENT] AS ' ',
    [REMISE] AS '',
    cout_of_fctr AS '',
    [TOTAL_FACURE_TTC] - cout_of_fctr AS '',
    TB_INFO_CAISSIER.NAME + ' ' + TB_INFO_CAISSIER.PRENAME AS ''
FROM 
    [dbo].[TB_FACTURE_VENTE]
INNER JOIN 
    TB_CLIENT ON TB_CLIENT.id = TB_FACTURE_VENTE.ID_CLIENT
INNER JOIN 
    TB_INFO_CAISSIER ON TB_INFO_CAISSIER.id = TB_FACTURE_VENTE.ID_CAISSIER
INNER JOIN 
    TB_USER ON TB_USER.id = TB_FACTURE_VENTE.ID_USER
INNER JOIN 
    TB_OUVERIER ON TB_OUVERIER.id = TB_USER.ID_OUVERIER
WHERE 
    DATE = @date;




