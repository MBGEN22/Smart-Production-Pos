CREATE PROCEDURE [dbo].[InsertIntoLivraisonVente]
    @DATE datetime,
    @SENDER nvarchar(50),
    @RECUPIRATEUR nvarchar(50),
    @TYPE_LIV nvarchar(50),
    @PRICE_LIV money,
    @INFORMATION_CHAUFEURE nvarchar(50),
    @MATRICULE nvarchar(50),
    @ID_USER int,
    @ID_CAISSIER int,
    @id_Facture nvarchar(50)
AS
BEGIN
    INSERT INTO [dbo].[TB_LIVRAISON_VENTE]
           ([DATE]
           ,[SENDER]
           ,[RECUPIRATEUR]
           ,[TYPE_LIV]
           ,[PRICE_LIV]
           ,[INFORMATION_CHAUFEURE]
           ,[MATRICULE]
           ,[ID_USER]
           ,[ID_CAISSIER]
           ,[id_Facture])
     VALUES
           (@DATE
           ,@SENDER
           ,@RECUPIRATEUR
           ,@TYPE_LIV
           ,@PRICE_LIV
           ,@INFORMATION_CHAUFEURE
           ,@MATRICULE
           ,@ID_USER
           ,@ID_CAISSIER
           ,@id_Facture)
END

















