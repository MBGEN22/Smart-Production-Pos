/****** Object:  StoredProcedure [dbo].[GET_CAISSE_PRODUIT_REVENT]    Script Date: 04/05/2025 15:22:29 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO



CREATE PROC [dbo].[GET_CAISSE_PRODUIT_REVENT]
AS
SELECT [TB_ACHAT_PRODUIT_REVENT_CAISSE].[ID]
      ,TB_Produit_revente.CodeBarre as ' '
	  ,TB_Produit_revente.designation as ' '
      ,[QUANTITE] as''
	  ,[QUANTITE_BEFOR] as'   '
      ,[QUANTITE_AFTER] as'   '
      ,[TB_ACHAT_PRODUIT_REVENT_CAISSE].[PRICE_ACHAT_HT] as'  HT'
      ,[TB_ACHAT_PRODUIT_REVENT_CAISSE].[TVA] as'TVA'	    
      ,[TB_ACHAT_PRODUIT_REVENT_CAISSE].[PRICE_ACHAT_TTC] as'  TTC'
      ,[TB_ACHAT_PRODUIT_REVENT_CAISSE].[TOTAL_HT] as'  HT'
      ,[TB_ACHAT_PRODUIT_REVENT_CAISSE].[TOTAL_TTC] as'  TTC'
      ,[REMARQUE] as''
      ,[ID_FACTURE] as' '
      ,[TYPE_FACTURE] as ' '
  FROM [dbo].[TB_ACHAT_PRODUIT_REVENT_CAISSE]
  INNER JOIN  TB_Produit_revente ON  TB_Produit_revente.CODEBARRE = [TB_ACHAT_PRODUIT_REVENT_CAISSE].CODE_PRODUIT

















