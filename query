GO



CREATE proc [dbo].[add_Client]
@NAME     NVARCHAR(50),
@PRENAME  NVARCHAR(50),
@ADRESSE  NVARCHAR(50),
@PHONE	  NVARCHAR(50),
@FAX	  NVARCHAR(50),
@EMAIL 	  NVARCHAR(50),
@SOLD_TOTAL  	 float,
@SOLD_PAYEE		 float,
@SOLD_NON_PAYES	 float,
@sold_verse float , 
@Max_credit float,
@NIF nvarchar(50),
@Rc nvarchar(50),
@Nic nvarchar(50),
@article nvarchar(50),
@original_name_prename nvarchar(50)
as
INSERT INTO [dbo].[TB_CLIENT]
           (
		    [NAME]
           ,[PRENAME]
		   ,ADRESSE
           ,[PHONE]
           ,[FAX]
           ,[EMAIL]
           ,[SOLD_TOTAL]
           ,[SOLD_PAYE]
           ,[SOLD_NON_PAYE]
           ,Sold_avance 
		   ,MAX_CREDIT
		   ,NIF  
		   ,RC
		   ,Nic
		   ,article  
		   ,originale_name_prename
		   )
     VALUES
           (
		   @NAME     ,
		   @PRENAME  ,
		   @ADRESSE  ,
		   @PHONE	 ,
		   @FAX	  	 ,
		   @EMAIL	 ,
		   @SOLD_TOTAL,
		   @SOLD_PAYEE,
		   @SOLD_NON_PAYES 	,
		   @sold_verse , 
		   @Max_credit,
		   @NIF,
		   @Rc,
		   @Nic,
		   @article,
		   @original_name_prename
		   )
















GO
