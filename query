CREATE proc [dbo].[get_tb_matier_premier]
@code_bare nvarchar(50)
as
SELECT [CODEBARRE]
      ,[DESIGNATION]
      ,[UNITE]
      ,[CATEGORIES]
      ,[STOCKES]
      ,[MARKE]
      ,[QUANTITE]
      ,[QUANTITE_REST]
      ,[QUANTITE_USE]
      ,[QUANTITE_PACKES]
      ,[PHOTO]
      ,[DATE_PREPARATION]
      ,[OBSERVATION]
      ,[STOCKES_MIN]
      ,[PRODUIT_DES_PIECES]
		   ,PRICE_HT
		   ,PRICE_TTC
		   ,TVA
  FROM [dbo].[TB_MATIER_PREMIER]
  where TB_MATIER_PREMIER.CODEBARRE = @code_bare
















