	  ,TB_FACTURE_ACHAT_PRODUIT_REVENT.DATE as'' 
      ,[QUANTITE] as''
      ,[PRICE_ACHAT_HT] as'  HT'
      ,[TVA] as'TVA'
      ,[PRICE_ACHAT_TTC] as'  TTC'
      ,[TB_ACHAT_PRODUIT_REVENT].[TOTAL_HT] as'  HT'
      ,[TOTAL_TTC] as'  TTC' 
      ,[ID_FACTURE] as' '
      ,[QUANTITE_BEFOR] as'   '
      ,[QUANTITE_AFTER] as'   '
  FROM [dbo].[TB_ACHAT_PRODUIT_REVENT]
  inner join TB_FACTURE_ACHAT_PRODUIT_REVENT on TB_FACTURE_ACHAT_PRODUIT_REVENT.NMR_FCTR = TB_ACHAT_PRODUIT_REVENT.ID_FACTURE
