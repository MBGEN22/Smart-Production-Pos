      ,[designation]
      ,[reference]
      ,[id_categories]
      ,[id_stocke]
      ,[id_marque]
      ,[id_unite]
      ,[id_taille]
      ,[id_color]
      ,[id_favoire]
      ,[date_expiration]
      ,[price_achat_HT]
      ,[price_achat_TTC]
      ,[TVA]
      ,[Quantite_TOTAL]
      ,[price_vente1]
      ,[price_vente2]
      ,[price_min]
      ,[vente_apres_expiration]
      ,[stocke_negatif]
      ,[Quantite_vente]
      ,[Quantite_rest]
      ,[Quantite_dechet]
      ,[Photo]
      ,[QT_DANS_PACK]
  FROM [dbo].[TB_Produit_revente]
  where CodeBarre = @CodeBarre















