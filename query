CREATE PROC [dbo].[GET_PRINCIPE_INVENTAIRE]
AS
SELECT [NMR_INVENTAIR] as' '
      ,[DATE] as''
      ,[HEURE] as''
      ,[TOTAL_THEORIQUE] as'  '
      ,[TOTAL_PHYSIQUE] as'  '
      ,[MONTANT_ECART] as''
