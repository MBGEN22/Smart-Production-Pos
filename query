CREATE proc [dbo].[get_facture_detailles]
as
SELECT [NMR_FACTURE] as' '
      ,[DATE] as''
      ,[HEURE] as''
      ,TB_CLIENT.NAME +' '+TB_CLIENT.PRENAME  as''
      ,[TOTAL_FACURE_TTC] as'  TTC'
      ,[TOTAL_FACTURE_HT] as'  HT' 
      ,[TVA] as'TVA'
      ,[OLD_SOLD] as' '
      ,[VERSEMENT] as' '
      ,[NEW_SOLD] as' '
      ,[REMISE] as''
      ,[POURSENTAGE_REMISE] as' '
      ,[COUNT_ARTICLE] as' '
      ,[TB_FACTURE_VENTE].[REMARQUE] as''
      ,[ETAT] as''
      ,TB_OUVERIER.NAME+' '+TB_OUVERIER.PRENAME as''
      ,TB_INFO_CAISSIER.NAME+' '+TB_INFO_CAISSIER.PRENAME as''
  FROM [dbo].[TB_FACTURE_VENTE]
  inner join TB_CLIENT on TB_CLIENT.id = TB_FACTURE_VENTE.ID_CLIENT
  inner join TB_INFO_CAISSIER on TB_INFO_CAISSIER.id = TB_FACTURE_VENTE.ID_CAISSIER
  inner join TB_USER on TB_USER.id = TB_FACTURE_VENTE.ID_USER
  inner join TB_OUVERIER on TB_OUVERIER.id = TB_USER.ID_OUVERIER

















