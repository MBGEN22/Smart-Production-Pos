           ([CODE_BARRE_PRODUIT]
           ,[QUANTITE_VENTE]
           ,[PRIX_VENTE]
           ,[PRIX_TOTAL]
           ,[ID_FACTURE]
		   ,cout,
		   typee
		   )
     VALUES
           (
		    @CODE_BARRE_PRODUIT 
		   ,@QUANTITE_VENTE	 
		   ,@PRIX_VENTE		 
