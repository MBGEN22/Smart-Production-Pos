      ,[ID_PRODUIT] as' '
      ,[QUANTITE] as'' 
      ,[PRIX_ACHAT_HT] AS '  HT $',
       [TVA] as'TVA'
      ,[PRIX_ACHAT_TTC] AS '  TTC $'
  FROM [dbo].[TB_ACHAT]
  where ID_PRODUIT = @id_produit

















