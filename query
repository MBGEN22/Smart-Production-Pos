@Rc nvarchar(50),
@Nic nvarchar(50),
@article nvarchar(50),
@original_name_prename nvarchar(50)
as
UPDATE [dbo].TB_FOURNISSUER
   SET [NAME] =          @NAME    		
      ,[PRENAME] =  	 @PRENAME		
      ,[PHONE] =  		 @PHONE			
      ,[FAX] =  		 @FAX			
      ,[EMAIL] =  		 @EMAIL			
      ,[SOLD_TOTAL] =  	 @SOLD_TOTAL		
      ,[SOLD_PAYE] =  	 @SOLD_PAYEE		
      ,[SOLD_NON_PAYE] = @SOLD_NON_PAYES   
      ,[ADRESSE] =  	 @ADRESSE 
	  ,NIF = @NIF
	  ,RC = @Rc
	  ,Nic = @Nic
	  ,article = @Article
	  ,originale_name_prename = @original_name_prename
 WHERE TB_FOURNISSUER.ID=@ID

















