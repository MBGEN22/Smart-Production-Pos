create proc [dbo].[get_my_fctr_data]
@nmr_fctr nvarchar(50)
as
SELECT [NMR_FACTURE]
      ,[DATE]
      ,[HEURE]
      ,[ID_CLIENT]
      ,[TOTAL_FACURE_TTC]
      ,[TOTAL_FACTURE_HT]
      ,[TVA]
      ,[OLD_SOLD]
      ,[VERSEMENT]
      ,[NEW_SOLD]
      ,[REMISE]
      ,[POURSENTAGE_REMISE]
      ,[COUNT_ARTICLE]
      ,[REMARQUE]
      ,[ETAT]
      ,[ID_USER]
      ,[ID_CAISSIER]
      ,[cout_of_fctr]
      ,[tmbre]
      ,[pricearabe]
  FROM [dbo].[TB_FACTURE_VENTE]
  where NMR_FACTURE =@nmr_fctr




