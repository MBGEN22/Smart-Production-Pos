      ,TB_FOURNISSUER.NAME + ' ' +TB_FOURNISSUER.PRENAME  as'  '
      ,[PRICE] as''
      ,[CREDIT_OLD] as' '
      ,[CREDIT_NEW] as' '
      ,[REMARQUE] as''
  FROM [dbo].[TB_HISTORIQUE_FOURNISSEUR]
