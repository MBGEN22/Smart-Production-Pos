  FROM [dbo].[TB_PRINCIPALE_INVENTAIRE]
	inner join TB_USER on TB_USER.ID = TB_PRINCIPALE_INVENTAIRE.ID_USER
	inner join TB_OUVERIER on TB_OUVERIER.ID = TB_USER.ID_OUVERIER


