--++++++++++++++++++++++++++++++++
create PROC [dbo].[ADD_QUANTITE_DECHET_HISTORIQUE]
 @DATE			date
,@TIME			time(7)
,@CODE_BARRE	nvarchar(50)
,@QUANTITE		float
,@LACAUSE		nvarchar(max)
,@ID_USER		int
AS
INSERT INTO [dbo].[TB_HISTORIQUE_DECHET]
           ([DATE]
           ,[TIME]
           ,[CODE_BARRE]
           ,[QUANTITE]
           ,[LACAUSE]
           ,[ID_USER])
     VALUES
           (
		     @DATE			
			,@TIME			
			,@CODE_BARRE	
			,@QUANTITE		
			,@LACAUSE		
			,@ID_USER		
		   )












