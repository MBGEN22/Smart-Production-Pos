CREATE proc [dbo].[get_table_dechet]
as  
SELECT [ID] as'ID'
      ,TB_MATIER_PREMIER.DESIGNATION as' '
      ,[QT_Dechet] as' '
	  ,price_u as' '
	  ,price_ttl as' '
      ,[etas] as''
	 
  FROM [dbo].[TB_DECHET_REClage]
  inner join TB_MATIER_PREMIER on TB_MATIER_PREMIER.CODEBARRE = TB_DECHET_REClage.ID_MATIER

















GO
/****** Object:  StoredProcedure [dbo].[GET_TABLE_HISTORIQUE_CLIENT]    Script Date: 04/05/2025 15:22:29 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


CREATE PROC [dbo].[GET_TABLE_HISTORIQUE_CLIENT]
AS
WITH CTE AS (
    SELECT
        [TB_HISTORIQUE_CLIENT].ID,
        [DATE],
        TB_CLIENT.NAME + ' ' + TB_CLIENT.PRENAME AS ,
        total_amounte AS _,
        [PRICE] AS _,
        total_amounte - [PRICE] AS ,
        [CREDIT_OLD] AS __,
        [CREDIT_NEW] AS __,
		[REMARQUE] AS ,
        ID_FACTURE AS _,
        LAG([TB_HISTORIQUE_CLIENT].[CREDIT_NEW], 1, 0) OVER (ORDER BY [TB_HISTORIQUE_CLIENT].ID) AS __
    FROM
        [dbo].[TB_HISTORIQUE_CLIENT]
    INNER JOIN
        TB_CLIENT ON TB_CLIENT.id = TB_HISTORIQUE_CLIENT.[ID_CLIENT]
)
SELECT
    CTE.ID AS 'ID',
    CTE.[DATE] AS '',
    CTE.,
    CTE._,
    CTE._,
    CTE.,
    CTE.__ AS ' ',
    CTE.__ + CTE. AS ' ', 
    CTE.,
    CTE._
FROM CTE;
GO
