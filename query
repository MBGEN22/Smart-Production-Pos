CREATE proc [dbo].[edit_fournisseur]
@ID		  INT,
@NAME     NVARCHAR(50),
@PRENAME  NVARCHAR(50),
@ADRESSE  NVARCHAR(50),
@PHONE	  NVARCHAR(50),
@FAX	  NVARCHAR(50),
@EMAIL 	  NVARCHAR(50),
@SOLD_TOTAL  	 float,
@SOLD_PAYEE		 float,
@SOLD_NON_PAYES	 float, 
