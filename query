create proc [dbo].[get_tb_matier]
@id_matier_premier nvarchar(50)
as 
SELECT [CODEBARRE]
      ,[DESIGNATION]
      ,[UNITE]
      ,[CATEGORIES]
      ,[STOCKES]
      ,[MARKE]
      ,[QUANTITE]
      ,[QUANTITE_REST]
      ,[QUANTITE_USE]
      ,[QUANTITE_PACKES]
      ,[PHOTO]
      ,[DATE_PREPARATION]
      ,[OBSERVATION]
      ,[STOCKES_MIN]
      ,[PRODUIT_DES_PIECES]
  FROM [dbo].[TB_MATIER_PREMIER]
  where [CODEBARRE] = @id_matier_premier

















