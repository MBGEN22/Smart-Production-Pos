create PROC [dbo].[get_facturation_on_state]
@Date date
AS
SELECT 
    [NMR_FACTURE] AS ' ',
    [DATE] AS '',
    CONVERT(VARCHAR(8), [HEURE], 108) AS '',
    TB_CLIENT.NAME + ' ' + TB_CLIENT.PRENAME AS '',
	Total_ttc AS '  TTC',
    [VERSEMENT] AS ' ',
    [REMISE] AS '',
    cout_fctr AS '',
    Total_ttc - cout_fctr AS '',
    TB_INFO_CAISSIER.NAME + ' ' + TB_INFO_CAISSIER.PRENAME AS ''
FROM 
    [dbo].TB_FACTURATION
INNER JOIN 
    TB_CLIENT ON TB_CLIENT.id = TB_FACTURATION.ID_CLIENT
INNER JOIN 
    TB_INFO_CAISSIER ON TB_INFO_CAISSIER.id = TB_FACTURATION.id_caissie
INNER JOIN 
    TB_USER ON TB_USER.id = TB_FACTURATION.ID_USER
INNER JOIN 
    TB_OUVERIER ON TB_OUVERIER.id = TB_USER.ID_OUVERIER
WHERE 
    DATE = @date;




GO
