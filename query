		     @DATE				
			,@HEURE				
			,@LIVREURE			
			,@RECEVEUR			
			,@ID_COMMANDE		
			,@VERSEMENT_AVANT	
			,@VERSEMENT_ICI		
			,@TOTAL_LIVRAISON	
			,@LIVRAISON_AVANT	
			,@LIVRAISON_ICI		
			,@CREDIT_AVANT		
			,@CREDIT_AFTER
			)


