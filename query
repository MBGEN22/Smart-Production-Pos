      ,[SOLD_NON_PAYE] =[SOLD_NON_PAYE]- @Solde_verse
      ,[ADRESSE] = [ADRESSE]
      ,[Sold_avance] = [Sold_avance]
      ,[MAX_CREDIT] = [MAX_CREDIT]
