	  ,TB_INFO_DE_LIV.[DATE] as''
      ,TB_INFO_DE_LIV.NOM_DE_LIV as ' ' 
	  ,TB_INFO_DE_LIV.NOM_DE_RECU as ' '
	  ,TB_INFO_DE_LIV.TYPE_DE_LIV as ' ' 
	  ,TB_INFO_DE_LIV.PRIC_DE_LIV as ' '
      ,TB_Commande.NOM_COMMANDE as ' '
	  ,TB_CLIENT.NAME +' ' + TB_CLIENT.PRENAME as '  '  
  FROM [dbo].TB_INFO_DE_LIV 
  INNER JOIN TB_Commande ON TB_Commande.ID = TB_INFO_DE_LIV.id_commande  	  
  INNER JOIN TB_CLIENT ON TB_CLIENT.ID = TB_Commande.ID_CLIENT 
  where TB_COMMANDE.ID_CLIENT = @ID_Client
        
















