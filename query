create proc [dbo].[search_TB_commande_by_client]
@ID_Client int 
as
SELECT [TB_COMMANDE].[ID] as' '
      ,[DATE] as''
      ,[NOM_COMMANDE] as' '
      ,TB_CLIENT.NAME+' '+TB_CLIENT.PRENAME as'  '
      ,[TYPE_COMMANDE] as' '
      ,[COUT_TOTAL] as' '
      ,[VERSEMENT] as' '
      ,[QTE] as''
      ,[PRICE_TOTAL_HT] as'  HT'
      ,[PRICE_TOTAL_TTC] as'  TTC'
      ,[TVA] as'TVA'
      ,[deadline_Date] as' '
      ,[etas] as''
	  ,REMARQUE as''
  FROM [dbo].[TB_COMMANDE]
  inner join TB_CLIENT on TB_CLIENT.ID = TB_COMMANDE.ID_CLIENT
  where 
  		  [TB_COMMANDE].ID_CLIENT  = @ID_Client
















