      ,[CATEGORIE]
      ,[COUT_DE_PRODUCTION]
      ,[QT_DANS_PACK]
      ,[QT_TOTAL]
      ,[QT_VENTE]
      ,[QT_REST]
      ,[QT_DECHET]
      ,[PRICE_VENTE_HT]
      ,[PRICE_VENTE_TTC]
      ,[TVA]
      ,[PRICE_GROS]
      ,[PRICE_MIN]
      ,[qt_alter]
  FROM [dbo].[TB_GESTION_PRODUIT_FINAUX]
  where CODE_BARRE = @code_barre

















