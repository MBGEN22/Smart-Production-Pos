      ,TB_FAVORIS.FAVOIRE as''
      ,[date_expiration] as'  '
      ,[price_achat_HT] as'  HT'
      ,[price_achat_TTC] as'  TTC'
      ,[TVA] as'TVA'
      ,[Quantite_TOTAL] as' ' 
      ,[price_vente1] as'  '
      ,[price_vente2] as'  '
      ,[price_min] as'  '
      ,[vente_apres_expiration] as'   '
      ,[stocke_negatif] as'  -'
      ,[Quantite_vente] as' '
      ,[Quantite_rest] as' '
      ,[Quantite_dechet] as' ' 
      ,[QT_DANS_PACK] as'  '
