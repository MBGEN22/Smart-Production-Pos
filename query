           ,[QUANTITE_REST]
           ,[QUANTITE_USE]
           ,[QUANTITE_PACKES]
           ,[PHOTO]
           ,[DATE_PREPARATION]
           ,[OBSERVATION]
           ,[STOCKES_MIN]
           ,[PRODUIT_DES_PIECES])
     VALUES
           (
		    @CODEBARRE			
			,@DESIGNATION		
			,@UNITE				
			,@CATEGORIES		
