CREATE PROC [dbo].[ADD_ON_CAISSE]
@ID_PRODUIT			nvarchar(50)
,@QUANTITE			float
,@QUANTITE_AFTER	float
,@PRIX_ACHAT_HT		money
,@TVA				float
,@PRIX_ACHAT_TTC	money
,@TOTAL_HT			money
,@TOTAL_TTC			money
,@REMARQUE			nvarchar(50)
,@ID_FACTURE		nvarchar(50)
,@Type_achat		nvarchar(50)
AS
INSERT INTO [dbo].[TB_ACHAT_CAISSE]
           ([ID_PRODUIT]
           ,[QUANTITE]
           ,[QUANTITE_AFTER]
           ,[PRIX_ACHAT_HT]
           ,[TVA]
           ,[PRIX_ACHAT_TTC]
           ,[TOTAL_HT]
           ,[TOTAL_TTC]
           ,[REMARQUE]
           ,[ID_FACTURE]
		   ,TYPE_achat)
     VALUES
           (
		     @ID_PRODUIT			
			,@QUANTITE			
			,@QUANTITE_AFTER	
			,@PRIX_ACHAT_HT		
			,@TVA				
			,@PRIX_ACHAT_TTC	
			,@TOTAL_HT			
			,@TOTAL_TTC			
			,@REMARQUE			
			,@ID_FACTURE	
			,@Type_achat	
		   )

















